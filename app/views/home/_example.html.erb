<%= link_to example_path(example) do %>
  <li>
    <div class="p-3 rounded-lg hover:bg-gray-100">
      <div class="bg-red-100 rounded-lg ring-1 ring-red-100">
        <%= image_tag example_image(example), class: "inset-o w-full h-full" %>
      </div>
      <span class="block mt-3 font-medium text-gray-90"><%= example.name %></span>
    </div>
  </li>
<% end %>
